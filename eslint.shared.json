{
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "error",
      2, {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "curly": [
      "error",
      "all"
    ],
    "require-yield": 0,
    "eol-last": ["error", "always"],
    "no-unused-vars": "error"
  }
}
