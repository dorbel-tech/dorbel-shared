{
  "name": "dorbel-shared",
  "version": "1.0.50",
  "description": "Shared module for dorbel services",
  "main": "src/index.js",
  "scripts": {
    "release": "scripts/release.sh",
    "test": "yarn run test:unit",
    "test:unit": "nyc --all mocha --timeout 10000 --require ./test/before-tests.js --recursive ./test",
    "lint": "eslint --ext .js -f table src",
    "watch": "yarn test -- -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "analytics-node": "^2.1.0",
    "auth0": "2.5.0",
    "bluebird": "^3.4.6",
    "bunyan": "~1.8.1",
    "bunyan-sumologic": "^1.2.0",
    "co": "^4.6.0",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "jwt-decode": "^2.1.0",
    "lodash": "~4.16.3",
    "mock-require": "^2.0.1",
    "newrelic": "^1.38.2",
    "nyc": "^10.1.2",
    "redis": "^2.6.3",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "sinon-as-promised": "^4.0.2",
    "sns.js": "1.0.2",
    "sqs-consumer": "3.4.0",
    "swagger-model-validator": "~2.1.2",
    "throng": "^4.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "co-mocha": "~1.1.3",
    "eslint": "~3.7.1",
    "hamjest": "~2.13.0",
    "mocha": "~3.1.0",
    "mock-fs": "^3.12.1",
    "sinon": "^1.17.6"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "json"
    ],
    "include": [
      "src/**/*.js"
    ],
    "report-dir": "./test/coverage"
  }
}
